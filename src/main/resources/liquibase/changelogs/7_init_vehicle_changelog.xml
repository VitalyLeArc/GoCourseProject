<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

<changeSet id="7" author="Vitaly">
    <createTable tableName="vehicles" schemaName="courseproject">
        <column autoIncrement="true" name="id" type="INTEGER">
            <constraints primaryKey="true" primaryKeyName="id"/>
        </column>
        <column name="type" type="ENUM('CAR','MOTO','BUS')"/>
        <column name="bodytype" type="ENUM('SPORT','SEDAN','HATCHBACK','ENDURO')"/>
        <column name="brand" type="enum('HONDA', 'MITSUBISHI', 'NISSAN', 'SUBARU', 'TOYOTA')"/>
        <column name="model" type="varchar(50)"/>
    </createTable>
</changeSet>


</databaseChangeLog>